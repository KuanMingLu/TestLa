target 'Tesla' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!
  # Pods for Tesla
  pod 'IPaAVPlayer'
  pod 'IPaURLResourceUI'
  pod 'IPaKeyChain'
  pod 'IPaStoryboardSegues'
  pod 'IPaSecurity'
  pod 'IPaLog'
  
end


target 'TeslaWatch WatchKit Extension' do
  use_frameworks!
  platform :watchos, '6.0'
  pod 'IPaURLResourceUI'
  pod 'IPaKeyChain'
  pod 'IPaSecurity'
  
end
